<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fl_cf_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".camera.CameraFragment">

    <androidx.camera.view.PreviewView
        android:id="@+id/pv_cf_view_finder"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <ImageView
        android:id="@+id/iv_cf_preview"
        android:layout_width="200dp"
        android:layout_height="200dp"/>

    <com.nex3z.examples.tflite.detect.bbox.BBoxContainerView
        android:id="@+id/bbcv_cf_bbox_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#1900ff00"/>

</FrameLayout>